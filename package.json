{
	"name": "monolith-rest",
	"version": "1.0.0",
	"description": "monolith lambda test",
	"main": "index.js",
	"scripts": {
		"lint": "node_modules/.bin/eslint .",
		"setup": "npm i && npx cross-env AWS_PROFILE=dev sls deploy",
		"auth": "npx cross-env AWS_PROFILE=dev bash -x ./scripts/auth.bash",
		"format": "prettier --write .",
		"getJWT": "npx cross-env AWS_PROFILE=dev npm run auth dev us-east-2 signup && npm run auth dev us-east-2 signin",
		"getAPIKey": "npx cross-env AWS_PROFILE=dev aws apigateway get-api-keys --name-query monolith-rest-api-dev-api-key --region us-east-2 --include-values --query 'items[0].value' --output text",
		"test": "jest",
		"test:coverage": "jest --coverage"
	},
	"devDependencies": {
		"@aws-sdk/client-dynamodb": "^3.341.0",
		"@aws-sdk/client-sqs": "^3.354.0",
		"@aws-sdk/lib-dynamodb": "^3.341.0",
		"@aws-sdk/util-endpoints": "*",
		"@serverless/compose": "^1.2.4",
		"aws-sdk": "*",
		"aws-sdk-client-mock": "*",
		"cross-env": "^7.0.3",
		"eslint": "*",
		"jest": "*",
		"prettier": "*",
		"serverless": "^3.15.2",
		"serverless-deployment-bucket": "^1.6.0",
		"serverless-iam-roles-per-function": "^3.2.0",
		"serverless-offline": "^13.2.0",
		"serverless-prune-plugin": "*",
		"serverless-webpack": "*",
		"webpack": "*"
	},
	"dependencies": {
		"crypto": "*"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"jest": {
		"testTimeout": 20000,
		"verbose": true,
		"collectCoverage": false,
		"testEnvironment": "node"
	}
}
